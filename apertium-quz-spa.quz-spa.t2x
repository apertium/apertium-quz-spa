<rule comment="REGLA: SN">
<pattern>
  <pattern-item n="SN"/>
</pattern>
<action>
<let><var n="prepos"/><lit v=""/></let>
<call-macro n="quita-GD-ND"><with-param pos="1"/></call-macro>
<call-macro n="caso-pr"><with-param pos="1"/></call-macro>
<out><!-- caso -->
  <var n="prepos"/>
  <chunk>
    <clip pos="1" part="lem"/>
    <clip pos="1" part="tags"/>
    <clip pos="1" part="chcontent"/>
  </chunk>
</out>
<let><var n="prepos"/><lit v=""/></let>
</action>
</rule>

