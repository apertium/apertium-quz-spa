    <def-cat n="SA">
      <cat-item tags="SA"/>
      <cat-item tags="SA.*"/>
    </def-cat>
    
    <def-cat n="SPRN-itg">
      <cat-item tags="SPRN-itg"/>
      <cat-item tags="SPRN-itg.*"/>
    </def-cat>
    
    <def-cat n="SPRN-pers">
