<rule comment="SN SP > SP SN">
<pattern>
	<pattern-item n="SN"/>
	<pattern-item n="SP"/>
</pattern>
<action>
<let><var n="prepos"/><lit v=""/></let>
<call-macro n="quita-GD-ND"><with-param pos="1"/></call-macro>
<choose>
<when><test><and>
	<not><equal>
		<clip pos="2" part="cas"/><lit-tag v="nom"/>
	</equal></not>
	<not><equal>
		<clip pos="2" part="cas"/><lit-tag v="loc"/>
	</equal></not>
	<not><equal>
		<clip pos="2" part="cas"/><lit-tag v=""/>
	</equal></not>
</and></test>
<call-macro n="caso-pr"><with-param pos="2"/></call-macro>
<out>
	<var n="prepos"/>
</out>
</when>
</choose>
<out>
	<chunk>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="tags"/>
		<clip pos="2" part="chcontent"/>
	</chunk><b/>
</out>
<choose><when>
	<test><equal> <!-- REVISAR -->
		<clip pos="1" part="pers_pos"/><lit v=""/>
	</equal></test>
	<out>
		<chunk>
			<lit v="art"/>
			<lit-tag v="DET"/>
			<clip pos="1" part="gen"/>
			<clip pos="1" part="nbr"/>
			<lit v="{^el"/>
			<lit-tag v="det"/>
			<lit-tag v="def"/>
			<lit-tag v="2"/>
			<lit-tag v="3"/>
			<lit v="$}"/>
		</chunk><b/>
	</out>
</when></choose>
<out>
	<chunk>
		<clip pos="1" part="lem"/>
		<clip pos="1" part="tags"/>
		<clip pos="1" part="chcontent"/>
	</chunk>
</out>
</action>
</rule>

