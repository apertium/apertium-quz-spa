          <equal>
            <clip pos="1" side="sl" part="inf-lim"/>
            <lit v=""/>
          </equal>
        </not>
      </and>
    </test>
    <out>
    <chunk name="verbo-ger" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-ger"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><lit-tag v="ger"/></tag>          <!-- waqaylla - llorando -->
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
