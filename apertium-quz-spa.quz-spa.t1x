<!-- poner una regla para cambiar p1in y p1ex por p1 (pl) + adj posesivos-->
<rule comment="PRONOMBRE PERSONAL">
<pattern>
    <pattern-item n="prn-pers"/>
</pattern>
<action>
<call-macro n="firstWord"><with-param pos="1"/></call-macro>
<call-macro n="pon-m"><with-param pos="1"/></call-macro>
<call-macro n="pon-sg"><with-param pos="1"/></call-macro>
<call-macro n="quita-nom"><with-param pos="1"/></call-macro>
<choose>
    <when>
        <test><and>
        <not><equal>
          <clip pos="1" side="sl" part="cas"/>
          <lit-tag v="nom"/>
        </equal></not>
        <not><equal>
          <clip pos="1" side="sl" part="cas"/>
          <lit v=""/>
        </equal></not>
    </and></test>
    <choose>
      <when>
        <test><and>
          <equal>
            <clip pos="1" side="sl" part="pers"/>
            <lit-tag v="p1"/>
          </equal>
          <equal>
            <clip pos="1" side="sl" part="nbr"/>
            <lit-tag v="sg"/>
          </equal>
        </and></test>
        <let>
          <clip pos="1" side="tl" part="lemh"/>
          <lit v="mí"/>
        </let>
      </when>
      <when>
        <test><and>
          <equal>
            <clip pos="1" side="sl" part="pers"/>
            <lit-tag v="p2"/>
          </equal>
          <equal>
            <clip pos="1" side="sl" part="nbr"/>
            <lit-tag v="sg"/>
          </equal>
        </and></test>
        <let>
          <clip pos="1" side="tl" part="lemh"/>
          <lit v="ti"/>
        </let>
      </when>
    </choose>
  </when>
</choose>
<out>
<chunk name="prn-pers" case="caseFirstWord">
        <tags>
            <tag><lit-tag v="SPRN-pers"/></tag>
            <tag><clip pos="1" side="tl" part="pers"/></tag>
            <tag><clip pos="1" side="tl" part="gen"/></tag>
            <tag><clip pos="1" side="tl" part="nbr"/></tag>
            <tag><var n="caso"/></tag>
            <!-- <tag><clip pos="1" side="sl" part="pers_pos"/></tag> refuerzo nuqa(lla)y -->
        </tags>
    <lu>
        <clip pos="1" side="tl" part="lemh"/>
    <lit-tag v="prn.tn"/>
    <lit-tag v="2"/>
    <lit-tag v="3"/>
    <lit-tag v="4"/>
        <clip pos="1" side="tl" part="lemq"/>
    </lu>
</chunk>
</out>
</action>
</rule>
 
<rule comment="PRONOMBRE"> <!-- sin prn.pers -->
<pattern>
	<pattern-item n="prn"/>
</pattern>
<action>
<call-macro n="firstWord"><with-param pos="1"/></call-macro>
<call-macro n="pon-GD-ND"><with-param pos="1"/></call-macro>
<call-macro n="quita-nom"><with-param pos="1"/></call-macro>
<choose> <!-- quita acc si el prn no es de los que toman la prep. "a" -->
  <when>
    <test>
      <not>
        <or> <!-- AND ??? -->
            <in caseless="yes">
                <clip pos="1" side="tl" part="lem"/>
                <list n="prn-anim_es"/>
            </in>
            <in caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <list n="sapan-kikin"/>
            </in>
            <and>
                <in caseless="yes">
                    <clip pos="1" side="sl" part="lem"/>
                    <list n="prn-anim_qu"/>
                </in>
                <or>
                    <equal>
                        <clip pos="1" side="sl" part="pers_pos"/>
                        <clip pos="1" side="sl" part="pers_pos-pl"/>
                    </equal>
                    <not>
                        <equal>
                            <clip pos="1" side="sl" part="pers_pos"/>
                            <lit v=""/>
                        </equal>
                    </not>
                </or>
            </and>
        </or>
      </not>
    </test>
    <call-macro n="quita-acc"><with-param pos="1"/></call-macro>
  </when>
  <otherwise>
    <let>
        <var n="caso"/>
        <clip pos="1" side="sl" part="cas"/>
    </let>
  </otherwise>
</choose>
<choose>
  <when>
    <test>
      <or>
          <in caseless="yes">
              <clip pos="1" side="sl" part="lem"/>
              <list n="sapan-kikin"/>
          </in>
          <and>
              <equal caseless="yes">
                  <clip pos="1" side="sl" part="lem"/>
                  <lit v="pipas"/>
              </equal>
              <equal>
                  <clip pos="1" side="sl" part="cas"/>
                  <lit-tag v="gen"/>
              </equal>
          </and>
      </or>
    </test>
    <out>
      <chunk name="adj" case="caseFirstWord">
          <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="gen"/></tag>
              <tag><clip pos="1" side="tl" part="nbr"/></tag>
              <tag><var n="caso"/></tag>
              <tag><clip pos="1" side="tl" part="pers_pos"/></tag>
          </tags>
          <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_adj"/>
              <lit-tag v="2"/>
              <lit-tag v="3"/>
              <clip pos="1" side="tl" part="lemq"/>
          </lu>
      </chunk>
    </out>
  </when>
  <otherwise>
    <out>
      <chunk name="prn" case="caseFirstWord">
          <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="gen"/></tag>
              <tag><clip pos="1" side="tl" part="nbr"/></tag>
              <tag><var n="caso"/></tag>
              <tag><clip pos="1" side="tl" part="pers_pos"/></tag>
          </tags>
          <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_prn-tn-itg"/>
              <lit-tag v="2"/>
              <lit-tag v="3"/>
              <clip pos="1" side="tl" part="lemq"/>
          </lu>
      </chunk>
    </out>
  </otherwise>
</choose>
</action>
</rule>
 
<rule comment="VERBO">
<pattern>
  <pattern-item n="verb"/>
</pattern>
<action>
<call-macro n="firstWord"><with-param pos="1"/></call-macro>
<call-macro n="remplaza-tiempo"><with-param pos="1"/></call-macro>
<call-macro n="remplaza-p1in-ex"><with-param pos="1"/></call-macro>
<call-macro n="remplaza-verb-sp"><with-param pos="1"/></call-macro>
<choose>
  <when>          <!-- TIEMPOS, CONDICIONAL, IMPERATIVO -->
    <test>
      <not>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <clip pos="1" side="sl" part="Subord"/>
        </equal>
      </not>
    </test>
    <out>
    <chunk name="verbo" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><clip pos="1" side="tl" part="TmpMod"/></tag>
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
        <clip pos="1" side="tl" part="lemh"/>
        <clip pos="1" side="tl" part="a_verb"/>
        <lit-tag v="3"/>
        <lit-tag v="4"/>
        <lit-tag v="5"/>
        <clip pos="1" side="tl" part="lemq"/>
      </lu>
    </chunk>
    </out>
  </when>
  <when>          <!-- GERUNDIO (sujetos idénticos) -->
    <test>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <lit-tag v="ger"/>
        </equal>
    </test>
    <out>
    <chunk name="verbo-ger" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-ger"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><clip pos="1" side="tl" part="Subord"/></tag>          <!-- POR AHORA (andinismo) - cuando, si -->
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
        <clip pos="1" side="tl" part="lemh"/>
        <clip pos="1" side="tl" part="a_verb"/>
        <lit-tag v="3"/>
        <clip pos="1" side="tl" part="lemq"/>
      </lu>
    </chunk>
    </out>
  </when>
  <when>          <!-- CONSECUTIVO -->
    <test>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <lit-tag v="cons"/>
        </equal>
    </test>
    <out>
    <chunk name="verbo-ger" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-ger"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><lit-tag v="ger"/></tag>          <!-- POR AHORA - mejor par + inf? -->
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
        <clip pos="1" side="tl" part="lemh"/>
        <clip pos="1" side="tl" part="a_verb"/>
        <lit-tag v="3"/>
        <clip pos="1" side="tl" part="lemq"/>
      </lu>
    </chunk>
    </out>
  </when>
  <when>          <!-- GERUNDIO (sujetos diferentes) -->
    <test>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <lit-tag v="ger2"/>
        </equal>
    </test>
    <out>
    <chunk name="verbo-ger" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-ger"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><lit-tag v="ger"/></tag>          <!-- POR AHORA - cuando, si -->
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
        <clip pos="1" side="tl" part="lemh"/>
        <clip pos="1" side="tl" part="a_verb"/>
        <lit-tag v="3"/>
        <clip pos="1" side="tl" part="lemq"/>
      </lu>
    </chunk>
    </out>
  </when>
  <when>          <!-- PARTICIPIO PASADO -->
    <test>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <lit-tag v="pp"/>
        </equal>
    </test>
    <choose>
      <when> <!-- poner número ND si pasa sin personas, género no pasa nunca - GD -->
        <test>           <!-- pero es según el objeto, o concordancia con N -->
          <equal>
            <clip pos="1" side="sl" part="nbr"/>
            <lit v=""/>
          </equal>
        </test>
        <let>
            <var n="numero"/>
            <lit-tag v="ND"/>
        </let>
      </when>
      <otherwise>
        <let>
            <var n="numero"/>
            <clip pos="1" side="tl" part="nbr"/>
        </let>
      </otherwise>
    </choose>
    <out>
    <chunk name="verbo-pp" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-pp"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><lit-tag v="pp"/></tag>          <!-- POR AHORA - también or. subord -->
        <tag><lit-tag v="GD"/></tag>
        <tag><var n="numero"/></tag>
        <tag><clip pos="1" side="tl" part="cas"/></tag>
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
        <clip pos="1" side="tl" part="lemh"/>
        <clip pos="1" side="tl" part="a_verb"/>
        <lit-tag v="3"/>
        <lit-tag v="4"/>
        <lit-tag v="5"/>
        <clip pos="1" side="tl" part="lemq"/>
      </lu>
    </chunk>
    </out>
  </when>
  <when>          <!-- INFINITIVO -->
    <test>
      <and>
        <equal>
          <clip pos="1" side="sl" part="TMS"/>
          <lit-tag v="inf"/>
        </equal>
        <equal>
          <clip pos="1" side="sl" part="inf-lim"/>
          <lit v=""/>
        </equal>
      </and>
    </test>
    <out>
    <chunk name="verbo-inf" case="caseFirstWord">
      <tags>
        <tag><lit-tag v="SV-inf"/></tag>
        <tag><clip pos="1" side="tl" part="verb-sin-pron"/></tag>
        <tag><lit-tag v="inf"/></tag>          <!-- POR AHORA - ? - nombre ? -->
        <tag><clip pos="1" side="tl" part="pers"/></tag>
        <tag><clip pos="1" side="tl" part="nbr"/></tag>
        <tag><clip pos="1" side="tl" part="pers_obj"/></tag>
        <tag><clip pos="1" side="tl" part="verb-pron"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-tipo"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-rec"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr1"/></tag>
        <tag><clip pos="1" side="tl" part="q_verb-gr2"/></tag>
        <tag><clip pos="1" side="tl" part="clit-indep"/></tag>
        <tag><clip pos="1" side="tl" part="clit-orac"/></tag>
      </tags>
      <lu>
